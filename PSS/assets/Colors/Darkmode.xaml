<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    <Color x:Key="PrimaryColor">#0d1b2a</Color>
    <Color x:Key="ForeGroundColor"> #FFFFFF</Color>
    <Color x:Key="NavButtonColorAndOutline">#1d2d44</Color>
    <Color x:Key="ApperanceTextColor">#2F2736</Color>
    <Color x:Key="SettingsButton">#594570</Color>
    
    <SolidColorBrush x:Key="BackgroundBrush" Color="#2C2C2C"/>
    <SolidColorBrush x:Key="BorderBrush" Color="#555555"/>
    <SolidColorBrush x:Key="CheckedBrush" Color="#81C784"/>
    <SolidColorBrush x:Key="HoverBrush" Color="#3C3C3C"/>
    <SolidColorBrush x:Key="NavIsChecked" Color="#1d2d44"/>
    <SolidColorBrush x:Key="Nav" Color="#2e4a78"/>
    
    <SolidColorBrush x:Key="PrimaryBrush" Color="{StaticResource PrimaryColor}" />
    <SolidColorBrush x:Key="ForeGroundBrush" Color="{StaticResource ForeGroundColor}" />
    <SolidColorBrush x:Key="NavButtonColorAndOutlineBrush" Color="{StaticResource NavButtonColorAndOutline}" />
    <SolidColorBrush x:Key="ApperanceTextBrush" Color="{StaticResource ApperanceTextColor}" />
    <SolidColorBrush x:Key="SettingsButtonBrush" Color="{StaticResource SettingsButton}" />
</ResourceDictionary>